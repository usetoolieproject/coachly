# Bunny CDN Configuration
# Get these values from your Bunny.net account

# Bunny CDN API Key (from Account Settings -> API)
BUNNY_API_KEY=your_bunny_api_key_here

# Bunny Stream Library ID (from Stream -> Video Library -> Settings)
BUNNY_LIBRARY_ID=your_library_id_here

# Bunny Storage Zone Name (from Storage -> Zones)
BUNNY_STORAGE_ZONE=your_storage_zone_name

# Bunny Storage Password/API Key (from Storage -> Zone -> FTP & API Access)
BUNNY_STORAGE_PASSWORD=your_storage_password_here

# Bunny CDN Hostname for streaming (e.g., vz-xxxxx.b-cdn.net)
# Find this in Stream -> Video Library -> Settings -> CDN Hostname
BUNNY_CDN_HOSTNAME=your_cdn_hostname_here

# Example values (replace with your actual values):
# BUNNY_API_KEY=a1b2c3d4-e5f6-7890-abcd-ef1234567890
# BUNNY_LIBRARY_ID=123456
# BUNNY_STORAGE_ZONE=coachly-videos
# BUNNY_STORAGE_PASSWORD=password123-456a-7890-bcde-f1234567890
# BUNNY_CDN_HOSTNAME=vz-12345678-123.b-cdn.net
